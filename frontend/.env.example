# ChainVoice Frontend - Environment Configuration Template
# Copy this file to .env and fill in your values

# Network Configuration
# Options: 'testnet' or 'mainnet'
VITE_NETWORK=mainnet

# Contract Addresses
# Your deployed smart contract addresses on Stacks blockchain
VITE_PROFILES_CONTRACT_ADDRESS=YOUR_PROFILE_CONTRACT_ADDRESS
VITE_MESSAGES_CONTRACT_ADDRESS=YOUR_MESSAGES_CONTRACT_ADDRESS

# Contract Names
VITE_PROFILES_CONTRACT_NAME=profiles
VITE_MESSAGES_CONTRACT_NAME=messages

# API Configuration (Optional - for future use)
# VITE_API_URL=https://api.chainvoice.com

